{
  "name": "flatsheet-editor",
  "description": "an editor for tabular data",
  "version": "0.0.0",
  "repository": {
    "url": "git://github.com/flatsheet/flatsheet-editor.git"
  },
  "main": "index.js",
  "browser": "index.js",
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "scripts": {
    "test": "tape test.js",
    "bundle-css": "rework-npm style.css | myth | cleancss -o bundle/app.css",
    "watch-css": "gazer -p 'style.css' 'rework-npm style.css | myth > bundle/app.css'",
    "watch-js": "watchify -d index.js -o bundle/app.js -t brfs",
    "bundle-js": "browserify -t brfs index.js -o bundle/app.js",
    "bundle": "npm run bundle-js & npm run bundle-css",
    "watch": "npm run watch-js & npm run watch-css",
    "start": "serve & npm run watch"
  },
  "dependencies": {
    "closest": "0.0.1",
    "component-closest": "^0.1.4",
    "component-emitter": "^1.1.3",
    "domify": "^1.3.1",
    "domquery": "^1.2.0",
    "element-class": "^0.1.1",
    "json-2-csv": "^0.1.3",
    "jsonpretty": "0.0.1",
    "level-js": "^2.1.6",
    "levelup": "^0.18.5",
    "normalize-css": "^2.3.1",
    "siblings": "^1.0.1",
    "table-editor": "^1.0.1"
  },
  "devDependencies": {
    "brfs": "^1.1.1",
    "browserify": "^4.1.6",
    "clean-css": "^2.2.8",
    "gazer": "0.0.3",
    "myth": "^1.1.0",
    "rework-npm-cli": "^0.1.1",
    "serve": "^1.4.0",
    "tape": "^2.13.3",
    "uglify-js": "^2.4.13",
    "watchify": "^0.10.1"
  }
}
